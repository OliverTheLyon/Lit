@article{domingo_perales2019,
	title = {Viral quasispecies},
	volume = {15},
	issn = {1553-7404},
	url = {https://dx.plos.org/10.1371/journal.pgen.1008271},
	doi = {10.1371/journal.pgen.1008271},
	language = {en},
	number = {10},
	urldate = {2021-09-25},
	journal = {PLOS Genetics},
	author = {Domingo, Esteban and Perales, Celia},
	month = {oct},
	year = {2019},
	keywords = {read},
	pages = {e1008271},
	annote = {A comprehensive review of support for Quasi-Species theory.
              This paper covers he hsitorical basis and gives a comprehensive 
              coverage of evidence for viral QS. It also discusses the 
              following topics, Error-prone replication and mutant spectra,
              phenotypic reservoir, adaptive parameters, intra-mutant spectrum
              quasispecies memory, sequence space, population bottle neck,
              biological constranits, connections with viral pathogenesis, and
              QS and long term evolution.}
}

