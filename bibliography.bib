	volume = {17},
	issn = {1098-3600, 1530-0366},
	shorttitle = {Standards and guidelines for the interpretation of sequence variants},
	url = {http://www.nature.com/articles/gim201530},
	doi = {10.1038/gim.2015.30},
	language = {en},
	number = {5},
	urldate = {2021-09-08},
